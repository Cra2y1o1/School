@{ 
    ViewBag.Title = "Список логинов-паролей";
    Layout = "~/Views/_Layout.cshtml";
}
@using School.Data.Models
@model IEnumerable<Person>


        <h2>All Persons:</h2>
        @{
            foreach (var person in Model)
            {
                <h2>
                    id: @person.id
                    Last name: @person.username
                    Last name: @person.password
                    Last name: @person.level
                </h2>
            }
        }

