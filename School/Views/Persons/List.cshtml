@using School.Data.Models
@model IEnumerable<Person>

<!DOCKTYPE html>

<html>
<head>
    <meta name="viewport" content="width-device-width" />
    <title>Persons</title>
</head>
<body>
    <h2>All Persons:</h2>
    @{
        foreach (var person in Model)
        {
            <h2>id: @person.id</h2>
            <h2>Last name: @person.lastName</h2>
        }
    }
</body>
</html>